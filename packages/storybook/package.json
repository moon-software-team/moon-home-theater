{
  "name": "@moon/storybook",
  "version": "1.0.0",
  "description": "The Moon Home Theater react components storybook",
  "keywords": [
    "storybook"
  ],
  "type": "module",
  "author": "Mallory Scotton <mallory.scotton@epitech.eu>",
  "homepage": "https://github.com/moon-software-team/moon-home-theater",
  "license": "MIT",
  "main": "src/index.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moon-software-team/moon-home-theater",
    "directory": "packages/storybook"
  },
  "bugs": {
    "url": "https://github.com/moon-software-team/moon-home-theater/issues"
  },
  "scripts": {
    "dev": "pnpm storybook dev -p 6006",
